{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2015,
    "sourceType": "module"
  },
  "rules": {
    "eqeqeq": ["error","always"],
    "curly": "error",
    "no-console": 0,
    "semi": ["error","always"],
    "quotes": ["error","single"],
    "indent": ["error", 2],
    "linebreak-style": ["error","unix"]
  }
}